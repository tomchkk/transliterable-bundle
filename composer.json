{
    "name": "tomchkk/transliterable-bundle",
    "description": "A Symfony bundle providing an embeddable class, `Transliterable`, to enable mapping of a Doctrine entity field to `_original` and `_transliteration` columns within an entity.",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "tomchkk",
            "email": "tpm.moore@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/orm": "^2.6",
        "doctrine/event-manager": "^1.0",
        "doctrine/persistence": "^1.1",
        "symfony/config": "^4.2",
        "symfony/dependency-injection": "^4.2",
        "symfony/http-kernel": "^4.2",
        "symfony/property-access": "^4.2"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Tomchkk\\TransliterableBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tomchkk\\TransliterableBundle\\Tests\\": "src/Tests/"
        }
    }
}
